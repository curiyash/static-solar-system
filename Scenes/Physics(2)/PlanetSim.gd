extends Node2D

#Camera

#Initialzie variables
var G = 6.67e-11
var m1 = 1.989e30
var m2 = 5.972e24
var d = 1.496e11

#Center of Mass
var r1 = -m2*d/(m1+m2)
var r2 = m1*d/(m1+m2)

# Q: what is Pm?
var p_1 = Vector2(r1, 0)
var p_2 = Vector2(r2, 0)
var p = [p_1, p_2]

#Initialize parameters
var h = 86400
var T = 365*h
var t = 0
var n = 366

#Angular velocity, Linear velocity
var omega = pow((G*(m1+m2)/pow(d, 3)), 0.5)

var v1 = r1*omega
var v2 = r2*omega

var vel = [Vector2(0, v1), Vector2(0, v2)]

#Initialize acc
var acc = [Vector2(), Vector2()]

#Force function
func force(pos, acc, m1, m2, G):
	#pos = p

	#relative vector
	# dp = [x, y]
	var dp = pos[0] - pos[1]

	var r = pow(pow(dp[0], 2) + pow(dp[1], 2), 0.5)
	
	acc[0][0] = - G * m2 *dp[0]/ pow(r, 3)
	acc[0][1] = - G * m2 *dp[1]/ pow(r, 3)
	
	acc[1][0] = - G * m1 *dp[0]/ pow(r, 3)
	acc[1][1] = - G * m1 *dp[1]/ pow(r, 3)
	
	return(acc)

#Leapfrog
func Leapfrog(pos, acc, vel, h, t):
	# 1/2 kick
	vel += acc * h/2.0

	# drift
	pos += vel * h

	#force
	acc = force(pos, acc, m1, m2, G)

	# 1/2 kick
	vel += acc * h/2.0
	t += h
	
	var parameters = [pos, acc, vel, t]
	return parameters

